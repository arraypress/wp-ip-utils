{
  "name": "arraypress/wp-utils-ip",
  "version": "1.0.0",
  "description": "A lean WordPress utility for IP address validation, CIDR operations, and GDPR-compliant anonymization",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-utils-ip/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\IPUtils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-utils-ip/issues",
    "source": "https://github.com/arraypress/wp-utils-ip"
  },
  "keywords": [
    "wordpress",
    "ip",
    "ipv4",
    "ipv6",
    "cidr",
    "validation",
    "anonymize",
    "gdpr",
    "arraypress"
  ]
}